{
  "expo": {
    "name": "ExamProject",
    "slug": "ExamProject",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.examproject",
      "intentFilters":  [{
        "action": "VIEW",
        "data":[{
          "host": "dev-qvr1pruh.eu.auth0.com",
          "pathPrefix": "/android/com.examproject/callback",
          "scheme": "com.examproject"
        }],
        "category": [
          "BROWSABLE",
          "DEFAULT"
        ]
        }]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
